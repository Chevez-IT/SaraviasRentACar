<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.saraviasrenacar.www.entities.ArquileresEntity" table="arquileres" schema="saravias_rentacar">
        <id name="arquilerId">
            <column name="arquiler_id" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="tituloArquiler">
            <column name="titulo_arquiler" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="descripcionArquiler">
            <column name="descripcion_arquiler" sql-type="text"/>
        </property>
        <property name="precioArquiler">
            <column name="precio_arquiler" sql-type="decimal(10,2)" precision="10" scale="2"/>
        </property>
        <property name="fechaInicio">
            <column name="fecha_inicio" sql-type="date"/>
        </property>
        <property name="fechaFinalizacion">
            <column name="fecha_finalizacion" sql-type="date"/>
        </property>
        <property name="estadoArquiler">
            <column name="estado_arquiler" sql-type="varchar(25)" length="25"/>
        </property>
        <property name="fechaCreacion">
            <column name="fecha_creacion" sql-type="timestamp"/>
        </property>
        <many-to-one name="vehiculosByVehiculoId" class="sv.saraviasrenacar.www.entities.VehiculosEntity">
            <column name="vehiculo_id"/>
        </many-to-one>
        <many-to-one name="clientesByClienteActual" class="sv.saraviasrenacar.www.entities.ClientesEntity">
            <column name="cliente_actual"/>
        </many-to-one>
        <many-to-one name="empleadosByGestorId" class="sv.saraviasrenacar.www.entities.EmpleadosEntity">
            <column name="gestor_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>