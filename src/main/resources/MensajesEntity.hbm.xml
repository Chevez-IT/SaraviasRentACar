<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.saraviasrenacar.www.entities.MensajesEntity" table="mensajes" schema="saravias_rentacar">
        <id name="mensajeId">
            <column name="mensaje_id" sql-type="varchar(12)" length="12"/>
        </id>
        <property name="mensaje">
            <column name="mensaje" sql-type="text"/>
        </property>
        <property name="fechaCreacion">
            <column name="fecha_creacion" sql-type="timestamp"/>
        </property>
        <property name="estadoMensaje">
            <column name="estado_mensaje" sql-type="varchar(25)" length="25"/>
        </property>
        <many-to-one name="usuariosByEmisorMensaje" class="sv.saraviasrenacar.www.entities.UsuariosEntity">
            <column name="emisor_mensaje"/>
        </many-to-one>
        <many-to-one name="usuariosByReceptorMensaje" class="sv.saraviasrenacar.www.entities.UsuariosEntity">
            <column name="receptor_mensaje"/>
        </many-to-one>
    </class>
</hibernate-mapping>