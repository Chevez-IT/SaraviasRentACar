<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.saraviasrenacar.www.entities.VentasEntity" table="ventas" schema="saravias_rentacar">
        <id name="ventaId">
            <column name="venta_id" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="tituloVenta">
            <column name="titulo_venta" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="descripcionVenta">
            <column name="descripcion_venta" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="precioVenta">
            <column name="precio_venta" sql-type="varchar(255)"/>
        </property>
        <property name="fechaCompraVenta">
            <column name="fecha_compra_venta" sql-type="date"/>
        </property>
        <property name="estadoVenta">
            <column name="estado_venta" sql-type="varchar(25)" length="25"/>
        </property>
        <property name="fechaCreacionVenta">
            <column name="fecha_creacion_venta" sql-type="timestamp"/>
        </property>
        <many-to-one name="vehiculosByVehiculoId" class="sv.saraviasrenacar.www.entities.VehiculosEntity">
            <column name="vehiculo_id"/>
        </many-to-one>
        <many-to-one name="clientesByCompradorVenta" class="sv.saraviasrenacar.www.entities.ClientesEntity">
            <column name="comprador_venta"/>
        </many-to-one>
        <many-to-one name="empleadosByGestorId" class="sv.saraviasrenacar.www.entities.EmpleadosEntity">
            <column name="gestor_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>